{
  "name": "@larascript-framework/larascript-framework",
  "type": "module",
  "version": "2.0.6",
  "description": "A powerful Node.js framework with built-in ORM, type safety, and developer-friendly APIs",
  "main": "index.js",
  "scripts": {
    "install:no-scripts": "pnpm install --ignore-scripts",
    "build": "tsc --skipLibCheck && npx tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "fix-imports": "node fix-imports.js",
    "fix-eslint": "npx eslint --fix src",
    "fix": "pnpm run fix-imports && pnpm run fix-eslint",
    "start": "node ./dist/app.js",
    "dev": "nodemon --watch src --ext ts --exec tsx ./src/app.ts",
    "tinker": "nodemon --watch src --ext ts --exec tsx ./src/tinker.ts",
    "postinstall": "pnpm run build",
    "test": "jest --runInBand --passWithNoTests",
    "test:watch": "jest --runInBand --watch --passWithNoTests",
    "test:coverage": "jest --runInBand --coverage --passWithNoTests",
    "test:all": "run-s --continue-on-error test:jest",
    "setup": "npx tsx ./src/setup.ts",
    "network:up": "cd ./docker && docker-compose -f docker-compose.network.yml up -d network-setup",
    "db:up": "pnpm db:postgres:up && pnpm db:mongodb:up",
    "db:down": "pnpm db:postgres:down && pnpm db:mongodb:down",
    "db:restart": "pnpm db:down && pnpm db:up",
    "db:postgres:up": "pnpm network:up && cd ./docker && docker-compose -f docker-compose.postgres.yml up --build -d postgres",
    "db:postgres:down": "cd ./docker && docker-compose -f docker-compose.postgres.yml down",
    "db:mongodb:up": "pnpm network:up && cd ./docker && docker-compose -f docker-compose.mongodb.yml up --build -d mongodb",
    "db:mongodb:down": "cd ./docker && docker-compose -f docker-compose.mongodb.yml down",
    "api:up": "pnpm network:up && cd ./docker && docker-compose -f docker-compose.api.yml up --build -d api",
    "api:down": "cd ./docker && docker-compose -f docker-compose.api.yml down && docker-compose -f docker-compose.network.yml down",
    "api:sh": "docker exec -it larascript-api /bin/bash",
    "tslint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "@larascript-framework/async-session": "workspace:*",
    "@larascript-framework/crypto-js": "workspace:*",
    "@larascript-framework/cast-js": "workspace:*",
    "@larascript-framework/larascript-acl": "workspace:*",
    "@larascript-framework/larascript-auth": "workspace:*",
    "@larascript-framework/larascript-auth-routes": "workspace:*",
    "@larascript-framework/larascript-collection": "workspace:*",
    "@larascript-framework/larascript-core": "workspace:*",
    "@larascript-framework/larascript-database": "workspace:*",
    "@larascript-framework/larascript-events": "workspace:*",
    "@larascript-framework/larascript-logger": "workspace:*",
    "@larascript-framework/larascript-mail": "workspace:*",
    "@larascript-framework/larascript-observer": "workspace:*",
    "@larascript-framework/larascript-storage": "workspace:*",
    "@larascript-framework/larascript-utils": "workspace:*",
    "@larascript-framework/larascript-validator": "workspace:*",
    "@larascript-framework/larascript-views": "workspace:*",
    "@larascript-framework/larascript-console": "workspace:*",
    "@larascript-framework/make": "workspace:*",
    "@larascript-framework/contracts": "workspace:*",
    "@larascript-framework/test-helpers": "workspace:*",
    "@larascript-framework/setup": "workspace:*",
    "@larascript-framework/larascript-http": "workspace:*",
    "@larascript-framework/compose": "workspace:*",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "collect.js": "^4.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "express-busboy": "^10.1.0",
    "express-ejs-layouts": "^2.5.1",
    "glob": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.9.0",
    "nodemailer": "^7.0.5",
    "pg": "^8.12.0",
    "resend": "^4.7.0",
    "sequelize": "^6.37.3",
    "tslint": "^6.1.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@larascript-framework/eslint-config": "workspace:*",
    "@larascript-framework/jest-config": "workspace:*",
    "@larascript-framework/tsconfig": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.10",
    "tsc-alias": "^1.8.10",
    "webpack": "^5.92.1"
  },
  "author": {
    "name": "Benjamin Shepherd",
    "email": "ben.shepherd@gmx.com",
    "url": "https://www.linkedin.com/in/benjamin-programmer/"
  },
  "license": "ISC"
}