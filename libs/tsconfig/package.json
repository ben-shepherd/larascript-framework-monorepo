{
    "name": "@larascript-framework/tsconfig",
    "type": "module",
    "version": "1.0.0",
    "main": "./tsconfig.json",
    "exports": {
        ".": "./tsconfig.json"
    },
    "referenceScripts": {
        "dev": "nodemon --watch src --ext ts --exec tsx ./src/app.ts",
        "build": "tsc --skipLibCheck && npx tsc-alias",
        "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\""
    },
    "devDependencies": {
        "@types/node": "^22.13.10",
        "typescript": "^5.8.2"
    }
}